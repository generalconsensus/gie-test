<article class="card card--spotlight">
  <h2 class="card__spotlight-type{{ dashboardtypeclass }}">{{ dashboardtype }}</h2>
  <div class="card__image">
    <a href="{{ url }}">{{> atoms-card-image }}</a>
  </div>
  <div class="card__content">
    <div class="card__type">content type</div>
    <h5 class="card__title"><a href="{{ url }}">{{ title }}</a></h5>
  </div>
  <a href="#" class="card__favorite is-favorited"></a>
</article>
