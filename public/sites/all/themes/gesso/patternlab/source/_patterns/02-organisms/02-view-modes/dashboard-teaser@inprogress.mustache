<article class="card">
  <h2 class="card__dashboard-type{{ dashboardtypeclass }}">{{ dashboardtype }}</h2>
  <div class="card__image">
    <a href="{{ url }}">{{> atoms-card-image }}</a>
  </div>
  <div class="card__content">
    <div class="card__type">content type</div>
    <h3 class="card__title"><a href="{{ url }}">{{ title }}</a></h3>
  </div>
  <a href="#" class="card__favorite"></a>
</article>
