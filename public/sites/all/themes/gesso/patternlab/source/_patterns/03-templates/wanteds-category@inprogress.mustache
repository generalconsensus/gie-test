{{> organisms-header }}
{{> organisms-navigation }}

<main id="main" class="main" role="main" tabindex="-1">
  <div class="region-content">

    <div class="l-panels-landing preface-empty title sidebar-empty main">
      <div class="l-preface layout-constrain"></div>
      <div class="l-title region-landing-title">
        <div class="layout-constrain">
          {{> atoms-landing-page-intro-wanteds }}
        </div>
      </div>
      <div class="l-wrapper region-landing-content">
        <div class="layout-constrain">
          
          <div class="l-main">

            <div class="pane pane--wanteds-search">
              <div class="pane__content">
                <form accept-charset="UTF-8">
                  <div class="form-item form-item--textfield">
                    <input type="text" placeholder="Search the Wanted ads..." class="form-text">
                  </div>
                  <input type="submit" value="Search" class="button button--submit">
                </form>
              </div>
            </div>
            
            <div class="pane">      
              <div class="pane__content">
                <div class="view view--needs-listing-pages">
                   {{# listItems.nine }}
                      <div class="views-row">
                        {{> organisms-wanteds-teaser }}
                      </div>
                    {{/ listItems.nine }}
                </div>
              </div>
            </div>
          </div>

        </div>
      </div>
    </div>

  </div>
</main>

{{> organisms-postscript }}
{{> organisms-footer }}
