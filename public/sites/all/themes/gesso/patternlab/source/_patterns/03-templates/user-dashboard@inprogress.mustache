{{> organisms-header }}
{{> organisms-navigation }}

<main id="main" class="main" role="main" tabindex="-1"
  <div class="region-content">
    <div class="region__inner">

      <div class="l-panels-detail">
        <div class="l-center layout-constrain">

          <div class="pane pane--dashboard-header-image">
            <div class="pane__content">
              <div class="dashboard-header">
                <div class="dashboard-header__profile-image profile-thumb__image-rounded">
                  <img src="{{ user.img-150-150.src }}">
                </div>
                <div class="dashboard-header__text">
                  <h2 class="dashboard-header__title">Your Exchange Dashboard</h2>
                  <h3 class="dashboard-header__welcome">WELCOME {{ user.name }}</h3>
                </div>
                <div class="dashboard-header__actions">
                  <a href="#" class="button button--update-profile">Update your profile</a>
                  <a href="#" class="button button--view-profile">View your profile</a>
                  <a href="#" class="button button--email">Email Preferences</a>
                </div>
              </div>
            </div>
          </div>

          <div class="pane pane--quicktabs-dashboard dashboard">
            <div class="pane__content">
              <div class="quicktabs-wrapper quicktabs-style-nostyle jquery-once-1-processed" id="quicktabs-dashboard">
                <ul class="quicktabs-tabs quicktabs-style-nostyle">
                  <li class="active"><a class="is-active quicktabs-loaded jquery-once-2-processed" href="#">What's New?</a></li>
                  <li><a class="is-active" href="#">Innovation Impacts</a></li>
                  <li><a class="is-active" href="#">Funding Status</a></li>
                  <li><a class="is-active" href="#">Network</a></li>
                </ul>
                <div id="quicktabs-container-dashboard" class="quicktabs_main quicktabs-style-nostyle">
                  <div class="quicktabs-tabpage">

                    <div class="l-panels-dashboard main sidebar bottom-left bottom-right">
                      <div class="l-wrapper">
                        <aside class="l-sidebar">
                          <div class="pane pane--dashboard-notification-center">
                            <div class="pane__content">
                              <div class="dashboard-notifications">
                                <h2>NOTIFICATIONS</h2>
                                <ul>
                                  <li><a href="/users/aklatsky">Alex Klatsky</a>, <a href="/users/vitraphigelu">vitraphigelu</a>, <a href="/users/aklatsk2gmailcom">alex klatsky</a> commented on your content</li>
                                  <li><a href="/users/vitraphigelu">vitraphigelu</a>, <a href="/users/frabusocriw">frabusocriw</a>, <a href="/users/testustosto">testustosto</a> follow you</li>
                                  <li><a href="/users/aklatsky">Alex Klatsky</a>, <a href="/users/aklatsk2gmailcom">alex klatsky</a>, <a href="/users/dmcdermott">Daniel McDermott</a> follows your content</li>
                                  <li>Find Innovations on the <a href="/innovations">Exchange</a>.</li>
                                </ul>
                              </div>
                            </div>
                          </div>
                        </aside>
                        <div class="l-main-wrapper">
                          <div class="l-main">

                            <div class="pane pane--dashboard-exchange-updates">
                              <div class="pane__content">
                                <h2 class="pane__title">EXCHANGE AT A GLANCE</h2>
                                <div class="view view--data-page-views view--data-points">
                                  <div class="views-row">
                                    <article class="node node--persona node--data view-mode-data">
                                      <div class="data-point"><a href="#">334 <span>Organizations</span></a></div>
                                    </article>
                                  </div>
                                  <div class="views-row">
                                    <article class="node node--persona node--data view-mode-data ">
                                      <div class="data-point"><a href="#">$349.4M <span>Available</span></a></div>
                                    </article>
                                  </div>
                                  <div class="views-row">
                                    <article class="node node--persona node--data view-mode-data">
                                      <div class="data-point"><a href="#">1636 <span>Collaborators</span></a></div>
                                    </article>
                                  </div>
                                  <div class="views-row">
                                    <article class="node node--persona node--data view-mode-data">
                                      <div class="data-point"><a href="#">3907 <span>Innovations</span></a></div>
                                    </article>
                                  </div>
                                </div>
                              </div>
                            </div>

                            <div class="pane pane--dashboard-featured-items">
                              <div class="pane__content">
                                <ul class="list-class">
                                  <li>

                                    <article class="card">
                                      <h2 class="card__dashboard-type">Exchange Updates</h2>
                                      <div class="card__image">
                                        <a href="{{ url }}">{{> atoms-card-image }}</a>
                                      </div>
                                      <div class="card__content">
                                        <div class="card__type">content type</div>
                                        <h3 class="card__title"><a href="{{ url }}">{{ title }}</a></h3>
                                      </div>
                                      <a href="#" class="card__favorite"></a>
                                    </article>

                                  </li>
                                </ul>
                              </div>
                            </div>

                          </div>
                          <div class="l-wrapper">
                            <div class="l-bottom-left">

                              <div class="pane pane--dashboard-new-users">
                                <h2 class="pane__title">NEW EXCHANGE USERS</h2>
                                <div class="pane__content">
                                  <div class="view view--dashboard-new-users">
                                    <div class="views-row">
                                      <div class="views-field views-field-nothing">
                                        <span class="field-content"><a href="#"><img title="Default image, no image supplied by the user." alt="Default image, no image supplied by the user." src="http://giexchange-dev.s3.amazonaws.com/styles/dashboard_profile_image/s3/random/user/iStock_000052855756.jpg?itok=bXp7YfdZ" typeof="foaf:Image"></a> Mike Dos  joined the Exchange <em class="placeholder">2 weeks 1 day</em> ago</span>
                                      </div>
                                    </div>
                                    <div class="views-row">
                                      <div class="views-field views-field-nothing">
                                        <span class="field-content"><a href="#"><img title="Default image, no image supplied by the user." alt="Default image, no image supplied by the user." src="http://giexchange-dev.s3.amazonaws.com/styles/dashboard_profile_image/s3/random/user/iStock_000050200156.jpg?itok=8D1X3BlN" typeof="foaf:Image"></a> Dashboard Test  joined the Exchange <em class="placeholder">3 weeks 1 day</em> ago</span>
                                      </div>
                                    </div>
                                    <div class="views-row">
                                      <div class="views-field views-field-nothing">
                                        <span class="field-content"><a href="#"></a>    joined the Exchange <em class="placeholder">3 weeks 3 days</em> ago</span>
                                      </div>
                                    </div>
                                    <div class="views-row">
                                      <div class="views-field views-field-nothing">
                                        <span class="field-content"><a href="#"></a>    joined the Exchange <em class="placeholder">3 weeks 3 days</em> ago</span>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </div>

                            </div>
                            <div class="l-bottom-right">

                              <div class="pane pane--dashboard-data-viz">
                                <h2 class="pane__title">Funding Counts Chart</h2>
                                <div class="pane__content">
                                  <div data-ng-controller="FundingCountController"><bar-chart></bar-chart></div>
                                </div>
                              </div>

                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                 </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

    </div>
  </div>
</main>

{{> organisms-postscript }}
{{> organisms-footer }}
